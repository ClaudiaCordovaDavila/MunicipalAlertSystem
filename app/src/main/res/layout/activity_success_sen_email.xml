<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/muni"
    tools:context="com.alumno.municipalalertsystem.SuccessSenEmailActivity">

    <TextView
        android:id="@+id/textView21"
        android:layout_width="298dp"
        android:layout_height="121dp"
        android:layout_marginBottom="38dp"
        android:text="A Email Has Been Sented"
        android:textAlignment="center"
        android:textColor="black"
        android:textSize="50sp"
        app:layout_constraintBottom_toTopOf="@+id/textView22"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintVertical_bias="0.904"
        app:layout_constraintHorizontal_bias="0.657" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="253dp"
        android:layout_height="116dp"
        android:text="Thank you for your patient"
        android:textAlignment="center"
        android:textColor="@android:color/white"
        android:textSize="35sp"
        tools:layout_editor_absoluteY="197dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="0.504" />

    <Button
        android:id="@+id/button10"
        android:layout_width="123dp"
        android:layout_height="70dp"
        android:layout_marginRight="0dp"
        android:onClick="emaillogin"
        android:text="Login"
        android:textSize="24sp"
        app:layout_constraintRight_toRightOf="@+id/textView22"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="7dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/textView22"
        app:layout_constraintVertical_bias="0.672" />
</android.support.constraint.ConstraintLayout>
